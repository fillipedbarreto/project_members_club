{
    "version": 2,
    "builds": [
        {
            "src": "./src/scripts/server.js",
            "use": "@vercel/node",
            "config": {
                "includeFiles": ["./server.json"]
            }
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "./src/scripts/server.js"
        }
    ]
}